CREATE TABLE "ATS001"."SY_BANKS" 
   (	"URID" NUMBER(10,0) DEFAULT 0 NOT NULL ENABLE, 
	"CODE" VARCHAR2(20) NOT NULL ENABLE, 
	"NAME" VARCHAR2(64) NOT NULL ENABLE, 
	"DIRECTBANKCODE" VARCHAR2(20), 
	"DESCRIPTION" VARCHAR2(512), 
	"UPTODATE" DATE, 
	"ISACTIVE" NUMBER(2,0) DEFAULT 1 NOT NULL ENABLE, 
	"DISPLAYORDER" NUMBER(10,0) DEFAULT 0 NOT NULL ENABLE, 
	"CREATEDON" DATE DEFAULT SYSDATE NOT NULL ENABLE, 
	"CREATEDBY" NUMBER(10,0) DEFAULT 0 NOT NULL ENABLE, 
	"LASTMODIFIEDON" DATE DEFAULT SYSDATE NOT NULL ENABLE, 
	"LASTMODIFIEDBY" NUMBER(10,0) DEFAULT 0 NOT NULL ENABLE, 
	"ROWVERSION" NUMBER(19,0) DEFAULT 1 NOT NULL ENABLE, 